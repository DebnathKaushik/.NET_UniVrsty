
@{
    ViewBag.Title = "Delete";
}

<h2>Delete Page</h2>
<p>Are you sure to delete @Model.Name product</p> 

<form method="post">
    <input type="hidden" name="Id" value="@Model.Id" />
    <input type="submit" name="choice" value="Yes" class="btn btn-danger" />
    <input type="submit" name="choice" value="No" class="btn btn-secondary" />
   
</form>

